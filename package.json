    "element-plus": "^2.3.5",
    "eruda": "^3.0.0",
    "hotkeys-js": "^3.10.2",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.1",
    "pinia": "^2.1.3",
    "qs": "^6.11.2",
    "sortablejs": "^1.15.0",
    "vconsole": "^3.15.0",
