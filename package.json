    "@types/lodash-es": "^4.17.7",
    "@types/nprogress": "^0.2.0",
    "@types/path-browserify": "^1.0.0",
    "@types/qs": "^6.9.7",
    "@types/sortablejs": "^1.15.1",
    "@unocss/preset-icons": "^0.50.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
